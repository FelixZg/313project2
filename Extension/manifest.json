{
  "name": "MemorizingChrome",
  "version": "0.1",
  "description": "An extension that will help you remember stuff",
  "permissions": ["activeTab", "storage", "http://localhost:3000/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "landingPage.html"
  },
  "manifest_version": 2
  
}